{
  "gemini": {
    "apiKey": "${VITE_GEMINI_API_KEY}",
    "model": "gemini-1.5-flash",
    "maxTokens": 1000,
    "temperature": 0.7,
    "rateLimitPerMinute": 60,
    "enabled": true
  },
  "recaptcha": {
    "siteKey": "${VITE_RECAPTCHA_SITE_KEY}",
    "version": "v3",
    "scoreThreshold": 0.5,
    "enabled": true
  },
  "firebase": {
    "projectId": "valde-tech",
    "appId": "1:934046717:web:1234567890abcdef",
    "apiKey": ""
  },
  "performance": {
    "enableLazyLoading": true,
    "enableCaching": true,
    "cacheMaxAge": 86400,
    "debounceDelay": 300,
    "throttleDelay": 1000
  },
  "pwa": {
    "enabled": true,
    "installPrompt": true,
    "offlineMode": true
  },
  "environment": "production",
  "debug": false
}
